			#pragma multi_compile_instancing

			// Lighting include is needed because of GI
			#include "Packages/com.unity.render-pipelines.universal/ShaderLibrary/Lighting.hlsl"
			#include "Packages/com.unity.render-pipelines.universal/ShaderLibrary/Core.hlsl"

			struct Attributes
			{
				float4 positionOS       : POSITION;
				float2 uv               : TEXCOORD0;
				UNITY_VERTEX_INPUT_INSTANCE_ID
			};

			struct Varyings
			{
				float4 posWorld : TEXCOORD0;
				float4 shadowCoord : TEXCOORD1;
				float4 vertex : SV_POSITION;

				UNITY_VERTEX_INPUT_INSTANCE_ID
				UNITY_VERTEX_OUTPUT_STEREO
			};

			CBUFFER_START(UnityPerMaterial)
			float _ShadowBrightness;
			CBUFFER_END

			Varyings vert(Attributes input)
			{
				Varyings output = (Varyings)0;

				UNITY_SETUP_INSTANCE_ID(input);
				UNITY_TRANSFER_INSTANCE_ID(input, output);
				UNITY_INITIALIZE_VERTEX_OUTPUT_STEREO(output);

				VertexPositionInputs vertexInput = GetVertexPositionInputs(input.positionOS.xyz);
				output.vertex = vertexInput.positionCS;
				output.posWorld = float4(TransformObjectToWorld(input.positionOS.xyz), 1.0);
				output.shadowCoord = TransformWorldToShadowCoord(output.posWorld);
				return output;
			}

			half4 frag(Varyings input) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID(input);
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX(input);

				half shadow = MainLightRealtimeShadow(input.shadowCoord);
				shadow = clamp(shadow + _ShadowBrightness, 0, 1);
				return float4(0, 0, 0, 1.0 - shadow * shadow);

			}
			ENDHLSL
		}
	}
		FallBack "Hidden/Universal Render Pipeline/FallbackError"
}
